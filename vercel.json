{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/(.*)",
      "use": "@vercel/static"
    },
    {
      "src": "page/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/home",
      "dest": "/page/home/index.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/page/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/page/$1"
    }
  ]
}
