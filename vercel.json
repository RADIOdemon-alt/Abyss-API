{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.js|.*\\.css|.*\\.mp4|.*\\.jpg|.*\\.png|.*\\.gif)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/public/pages/$1/index.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
